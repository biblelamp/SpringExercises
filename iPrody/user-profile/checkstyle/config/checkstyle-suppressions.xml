<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>
    <suppress checks="JavadocVariable" files="[A-Z][a-zA-Z0-9]*Test.java|Test[A-Z][a-zA-Z0-9]*.java"/>
    <suppress checks="MethodName" files="[A-Z][a-zA-Z0-9]*Test.java|Test[A-Z][a-zA-Z0-9]*.java"/>
    <suppress checks="HideUtilityClassConstructor" files="[A-Z][a-zA-Z0-9]*Application.java"/>
    <suppress checks="MagicNumber" files=".*Test\.java"/>
    <suppress checks="Javadoc" files="."/>
</suppressions>